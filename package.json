{
  "name": "point-lab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node --watch server/index.js",
    "dev:all": "concurrently -n server,web -c cyan,green \"npm:server\" \"npm:dev\"",
    "migrate:json": "node server/scripts/migrate-from-json.js",
    "seed:from-prod": "node server/scripts/seed-from-prod.js",
    "migrate:drop-slug": "node server/scripts/drop-topics-slug.js",
    "fly:login": "flyctl auth login",
    "fly:volume": "flyctl volumes create data --size 1 --region sin",
    "fly:set-origins": "flyctl secrets set ALLOWED_ORIGINS=$PAGES_ORIGIN",
    "fly:deploy": "flyctl deploy",
    "fly:status": "flyctl status",
    "fly:logs": "flyctl logs --since 1h"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-charts": "^8.17.0",
    "better-sqlite3": "^9.4.5",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "opencc-js": "^1.0.5",
    "phosphor-react": "^1.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "daisyui": "^5.4.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
